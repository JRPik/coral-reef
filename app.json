{
  "expo": {
    "name": "Coral Reef Project",
    "slug": "coral-reef-project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/icon.png",
    "splash": {
      "image": "./app/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions":[
        "ACCESS_BACKGROUND_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION", 
        "CAMERA"],
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
      
    },
    "web": {
      "favicon": "./app/assets/images/favicon.png"
    },
    "description": "A place where divers will be able to input and store their corals progress.",
    "githubUrl": "https://github.com/JRPik/coral-reef.git"
  }
}
